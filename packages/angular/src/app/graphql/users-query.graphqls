extend type Query {
  users(page: Int, size: Int, sortBy: String, sortDir: String, filter: String): UsersPage
  usersList(roleId: ID): [Users]
  user(id: ID): Users
}

type Users {
  id: ID
  userId: String
  password: String
  name: String
  division: String
  phone: String
  mail: String
  loginAttempts: Int
}

type UsersPage {
  totalElements: Int
  content: [Users]
}
