extend type Query {
  blocklyList(page: Int, size: Int, sortBy: String, sortDir: String, filter: String): BlocklyPage
  blockly(id: ID): Blockly
}

type Blockly {
  id: ID
  author: String
  registerDate: Date
  data: String
  dataName: String
  dataDetail: String
  finished: Boolean
  finishDate: Date
}

type BlocklyPage {
  totalElements: Int
  content: [Blockly]
}
