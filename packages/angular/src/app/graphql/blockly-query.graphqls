extend type Query {
  blocklyList(page: Int, size: Int, sortBy: String, sortDir: String, filter: String): BlocklyPage
  blockly(id: ID): Blockly
}

type Blockly {
  id: ID
  author: String
  registerDate: Date
  blockJson: String
  data: String
  dataName: String
  dataDetail: String
  pythonData: String
  comment: String
  finished: Boolean
  finishDate: Date
  used: Boolean
}

type BlocklyPage {
  totalElements: Int
  content: [Blockly]
}
