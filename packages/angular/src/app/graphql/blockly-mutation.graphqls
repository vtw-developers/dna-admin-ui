extend type Mutation {
  createBlockly(blockly: BlocklyInput): Blockly
  updateBlockly(blockly: BlocklyInput): Blockly
  deleteBlockly(id: ID): Blockly
}

input BlocklyInput {
  id: ID
  author: String
  registerDate: Date
  blockJson: String
  data: String
  dataName: String
  dataDetail: String
  pythonData: String
  comment: String
  finished: Boolean
  finishDate: Date
}
